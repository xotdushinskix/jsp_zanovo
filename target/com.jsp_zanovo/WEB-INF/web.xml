<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app

    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    id="Your_WebApp_ID"
    version="2.5">



  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>startServlet</servlet-name>
        <servlet-class>newjsp.StartServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>registrationServlet</servlet-name>
        <servlet-class>newjsp.RegistrationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>delimeterInfo</servlet-name>
        <servlet-class>newjsp.DelimeterInfoAfterRegisterServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>giveInfo</servlet-name>
        <servlet-class>newjsp.GiveInfoServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>singleInfo</servlet-name>
        <servlet-class>newjsp.GiveSingleInfoServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>fileDowload</servlet-name>
        <servlet-class>newjsp.FileDownload</servlet-class>
    </servlet>





    <servlet-mapping>
        <servlet-name>startServlet</servlet-name>
        <url-pattern>/start</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>registrationServlet</servlet-name>
        <url-pattern>/registration_page</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>delimeterInfo</servlet-name>
        <url-pattern>/delimeter_info</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>giveInfo</servlet-name>
        <url-pattern>/give_user_info</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>singleInfo</servlet-name>
        <url-pattern>/give_user_single_info</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>fileDowload</servlet-name>
        <url-pattern>/filedownload</url-pattern>
    </servlet-mapping>





</web-app>
